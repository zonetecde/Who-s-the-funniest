<UserControl x:Class="Who_s_the_funniest.uc.UC_Message"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Who_s_the_funniest.uc"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Margin="0,2,0,2">
    <Grid>
        <TextBlock x:Name="TextBlock_msg" FontSize="16" TextWrapping="Wrap">
            <Run Text="[" FontWeight="Bold"/><Run x:Name="Run_username" FontWeight="Bold"/><Run Text="]" FontWeight="Bold"/><Run Text=" : "/><Run Text="test" x:Name="Run_Message"/>
        </TextBlock>
        <TextBlock x:Name="TextBlock_ServerMsg" FontSize="16" TextWrapping="Wrap" TextAlignment="Center" FontStyle="Italic" Visibility="Hidden">
            <Run x:Name="Run_ServerMessage" Text="test"/>
        </TextBlock>
    </Grid>
</UserControl>
